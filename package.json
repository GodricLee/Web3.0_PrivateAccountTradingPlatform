    "@web3uikit/core": "^0.1.5",
    "@web3uikit/icons": "^0.4.1",
    "eslint-config-next": "^13.3.1",
    "ethers": "^5.6.9",
    "framer-motion": "^10",
    "moralis": "^2.18.4",
    "next": "^13.3.1",
    "next-auth": "^4.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.28.0",
    "speakeasy": "^2.0.0",
